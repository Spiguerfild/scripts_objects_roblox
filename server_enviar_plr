--servidor
local teleport_service = game:GetService("TeleportService")
local evento = game:GetService("ReplicatedStorage"):FindFirstChild("level_one_lobby")

evento.OnServerEvent:Connect(function(plr)

	local plr_ws = game:GetService("Workspace"):FindFirstChild(plr.Name)
	


	if(plr_ws~=nil) then
	
		local player = plr.Parent
		local char	= game:GetService("Workspace"):FindFirstChild(plr.Name)
	
		teleport_service:Teleport(id_do_mundo1),char) 

	end
	
	
end)
